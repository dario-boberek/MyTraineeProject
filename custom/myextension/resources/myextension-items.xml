<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">
    <itemtypes>
        <itemtype code="CarProduct"
                  jaloclass="training.my.jalo.CarProduct"
                  extends="Product"
                  autocreate="true"
                  generate="true">

            <attributes>
                <attribute qualifier="color"
                           type="java.lang.String">
                    <description>Initial String Field</description>
                    <persistence type="property"/>
                    <modifiers read="true" write="true" search="true" optional="true"/>
                </attribute>

                <attribute qualifier="weight"
                           type="java.lang.Double">
                    <defaultvalue>0</defaultvalue>
                    <description>Initial Double Field</description>
                    <persistence type="property"/>
                    <modifiers read="true" write="true" search="true" optional="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="User" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="age" type="java.lang.Integer">
                    <defaultvalue>0</defaultvalue>
                    <description>Initial Integer Field</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype generate="true"
                  code="CurrentNumberOfOrdersCronJob"
                  jaloclass="de.hybris.cronjobtutorial.jalo.CurrentNumberOfOrdersCronJob"
                  extends="CronJob"
                  autocreate="true">
        </itemtype>

    </itemtypes>
</items>